{
  "expo": {
    "name": "crowdfacture",
    "slug": "crowdfacture",
    "version": "1.0.0",

    "orientation": "portrait",
    "icon": "./assets/logo/CrowdfactureIcon.png",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/logo/Crowdfacture-ORANGE-PNG.png",
      "resizeMode": "contain",
      "backgroundColor": "#00040F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00040F"
      },
      "permissions": ["CAMERA","WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
