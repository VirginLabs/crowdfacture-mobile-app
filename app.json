{
  "expo": {
    "name": "crowdfacture",
    "slug": "crowdfacture",
    "version": "1.2.3",
    "owner": "sumobank",
    "orientation": "portrait",
    "icon": "./assets/logo/CrowdfactureIcon.png",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/logo/Crowdfacture-ORANGE-PNG.png",
      "resizeMode": "contain",
      "backgroundColor": "#00040F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.crowdfacture.crowdfacture-mobile",
      "buildNumber": "1.2.3",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let user put a photo in his profile page.",
        "NSFaceIDUsageDescription": "This app uses face id to authenticate user"
      }
    },
    "android": {
      "versionCode": 5,
      "package": "com.crowdfacture.crowdfacture_mobile",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Crowdfacture-ORANGE-PNG.png",
        "backgroundColor": "#00040F"
      },

      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.crowdfacture.crowdfacture_mobile",
      "permissions": ["CAMERA","WRITE_EXTERNAL_STORAGE","USE_BIOMETRIC","USE_FINGERPRINT"]
    },
    "web": {
      "favicon": "./assets/Crowdfacture-icon.ico"
    },
    "description": ""
  }
}
